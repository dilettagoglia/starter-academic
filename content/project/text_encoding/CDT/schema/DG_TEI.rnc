    ((dg_model.frontPart
      | dg_model.pLike.front
      | dg_model.pLike
      | dg_model.listLike
      | dg_model.global)*,
     ((dg_model.div1Like,
       (dg_model.frontPart | dg_model.div1Like | dg_model.global)*)
      | (dg_model.divLike,
         (dg_model.frontPart | dg_model.divLike | dg_model.global)*))?,
     (dg_model.divBottomPart,
      (dg_model.divBottomPart | dg_model.global)*)?),
    dg_att.global.attributes,
    dg_att.declaring.attributes,
    empty
  }
